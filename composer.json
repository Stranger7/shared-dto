{
  "name": "shared-dto",
  "version": "1.0.3",
  "description": "Shared DTO models compiler",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.3",
    "ext-yaml": "*",
    "ext-mbstring": "*",
    "yiithings/yii2-dotenv": "*",
    "symfony/yaml": "6.0.x-dev",
    "yiisoft/validator": "^3.0@dev"
  },
  "autoload": {
    "psr-4": {
      "dto\\": "dto/php",
      "DTOCompiler\\": "app/",
      "DTOCompiler\\tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
